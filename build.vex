      "id": "punctuation/html",
      "links": ["honeycomb/core", "punctuation/core"],
      "sources": ["src/html"],
      "version": "0.2.0",
      "docs": ["doc"]
    },
    {
      "name": "Punctuation ANSI",
      "id": "punctuation/ansi",
      "links": ["harlequin/core", "punctuation/core"],
      "sources": ["src/ansi"],
      "version": "0.2.0",
      "docs": ["doc"]
    }
  ]
